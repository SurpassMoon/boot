<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lz.boot.dao.UserMapper">

    <select id="selectById" resultType="com.lz.boot.entity.User" databaseId="mysql">
        select
            *
        from user
        where id = #{id,jdbcType=BIGINT}
    </select>

    <select id="selectById" resultType="com.lz.boot.entity.User" databaseId="oracle">
        select
            *
        from user
        where id = #{id,jdbcType=BIGINT}
    </select>
</mapper>
